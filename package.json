{
  "name": "proicons",
  "version": "4.9.0",
  "description": "A collection of 400+ modern and open-source icons",
  "main": "dist/proicons.js",
  "typings": "lib/proicons.d.ts",
  "type": "module",
  "module": "dist/proicons.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "dev": "webpack serve --mode=development --open",
    "build-icons": "node ./bin/build.js",
    "build": "webpack --mode=production --node-env=production",
    "docs:dev": "vitepress dev site/src --port 9000",
    "docs:build": "vitepress build site/src",
    "docs:preview": "vitepress preview site/src",
    "dev-build": "webpack --mode=development",
    "prepack": "node ./bin/publish.js"
  },
  "keywords": [
    "icons",
    "icon-pack",
    "js"
  ],
  "author": "ProCode Software",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.3.0",
    "@types/webpack": "^5.28.5",
    "@webpack-cli/generators": "^3.0.7",
    "ansi-colors": "^4.1.3",
    "chalk": "^5.3.0",
    "d3-delaunay": "^6.0.4",
    "html-webpack-plugin": "^5.6.0",
    "jimp": "^1.4.0",
    "prettier": "^3.3.3",
    "sharp": "^0.33.5",
    "svg2ttf": "^6.0.3",
    "svgo": "^3.3.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "vitepress": "^1.3.4",
    "webpack": "^5.93.0",
    "webpack-dev-server": "^5.0.4",
    "progress": "^2.0.3"
  }
}
