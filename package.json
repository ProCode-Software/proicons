{
    "name": "proicons",
    "version": "4.9.0",
    "description": "A collection of 400+ modern and open-source icons",
    "type": "module",
    "main": "dist/cjs/proicons.cjs",
    "main:umd": "dist/umd/proicons.cjs",
    "module": "dist/proicons.js",
    "typings": "lib/proicons.d.ts",
    "amdName": "proicons",
    "unpkg": "dist/umd/proicons.cjs",
    "jsdelivr": "dist/umd/proicons.cjs",
    "browser": "dist/umd/proicons.cjs",
    "files": [
        "dist",
        "lib"
    ],
    "exports": {
        ".": {
            "import": "./dist/esm/proicons.js",
            "require": "./dist/cjs/proicons.cjs",
            "types": "./lib/proicons.d.ts",
            "default": "./dist/umd/proicons.cjs"
        }
    },
    "workspaces": [
        "packages/*",
        "tools/*",
        "site"
    ],
    "repository": {
        "url": "ProCode-Software/proicons"
    },
    "homepage": "https://github.com/ProCode-Software/proicons",
    "bugs": "https://github.com/ProCode-Software/proicons/issues",
    "scripts": {
        "dev": "vite",
        "icons:build": "node ./bin/build.js",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "docs:dev": "vitepress dev site/src --port 9000",
        "docs:build": "vitepress build site/src",
        "docs:preview": "vitepress preview site/src",
        "prepack": "node ./bin/publish.js",
        "cli": "npm run test -w @proicons/cli"
    },
    "keywords": [
        "icons",
        "icon-pack",
        "js",
        "feather",
        "lucide",
        "fluent",
        "font awesome",
        "svg",
        "proicons",
        "procode"
    ],
    "author": "ProCode Software",
    "license": "MIT",
    "devDependencies": {
        "@twbs/fantasticon": "^3.0.0",
        "@types/node": "^22.3.0",
        "ansi-colors": "^4.1.3",
        "chalk": "^5.3.0",
        "d3-delaunay": "^6.0.4",
        "jimp": "^1.4.0",
        "oslllo-svg-fixer": "^5.0.0",
        "prettier": "^3.3.3",
        "progress": "^2.0.3",
        "sass-embedded": "^1.79.3",
        "sharp": "^0.33.5",
        "svg2ttf": "^6.0.3",
        "svgo": "^3.3.2",
        "typescript": "^5.5.4",
        "vite": "^5.4.5",
        "vite-plugin-html": "^3.2.2",
        "vitepress": "^1.3.4"
    },
    "packageManager": "pnpm@9.12.0+sha512.4abf725084d7bcbafbd728bfc7bee61f2f791f977fd87542b3579dcb23504d170d46337945e4c66485cd12d588a0c0e570ed9c477e7ccdd8507cf05f3f92eaca"
}
