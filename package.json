{
    "name": "proicons",
    "version": "4.13.1",
    "description": "A collection of 500+ modern and open-source icons",
    "type": "module",
    "workspaces": [
        "packages/*",
        "tools/*",
        "site"
    ],
    "repository": {
        "url": "ProCode-Software/proicons"
    },
    "homepage": "https://github.com/ProCode-Software/proicons",
    "bugs": "https://github.com/ProCode-Software/proicons/issues",
    "scripts": {
        "all:build-modules": "bun -r run icons:build-modules",
        "icons:build": "bun run --no-deprecation ./bin/build.ts",
        "icons:optimize": "bun run icons:build -o",
        "icons:build-frozen": "bun run icons:build -l",
        "icons:build-rebuild": "bun run icons:build -r",
        "icons:roblox-publish": "bun run --env-file=.env ./bin/publishToRoblox.ts",
        "icons:prune": "bun run ./bin/pruneFiles.ts",
        "font:codepoints": "bun run ./bin/generateCodepoints.ts",
        "font:build": "bun run icons:build -f -r",
        "generate-changelog": "bun run ./bin/generateChangelog.ts",
        "docs:dev": "bun --filter @proicons/site run docs:dev",
        "docs:build": "bun --filter @proicons/site run docs:build",
        "ci": "bun run -r ci"
    },
    "author": "ProCode Software",
    "license": "MIT",
    "devDependencies": {
        "@proicons/cli": "workspace:*",
        "@proicons/pixelfix": "workspace:*",
        "@proicons/svgo-plugins": "workspace:*",
        "@proicons/shared": "workspace:*",
        "@twbs/fantasticon": "^3.1.0",
        "@types/bun": "latest",
        "@typescript/native-preview": "7.0.0-dev.20250723.1",
        "ansi-colors": "^4.1.3",
        "axios": "^1.13.2",
        "form-data": "^4.0.5",
        "jsdom": "^27.4.0",
        "oslllo-svg-fixer": "^5.0.0",
        "piscina": "^5.1.4",
        "prettier": "^3.7.4",
        "prettier-plugin-svelte": "^3.4.1",
        "progress": "^2.0.3",
        "sharp": "^0.34.5",
        "svgo": "^4.0.0",
        "typescript": "^7.0.0-dev.20260111.1",
        "vite": "npm:rolldown-vite@latest"
    },
    "overrides": {
        "rolldown": "latest",
        "vite": "npm:rolldown-vite@latest",
        "typescript": "npm:@typescript/native-preview@latest"
    },
    "packageManager": "bun@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
