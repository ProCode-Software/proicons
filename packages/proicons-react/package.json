{
    "name": "@proicons/react",
    "version": "4.11.4",
    "description": "ProIcons icon library for ReactJS",
    "main": "dist/proicons-react.js",
    "module": "dist/proicons-react.js",
    "types": "dist/proicons-react.d.ts",
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist",
        "!dist/types"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "dev": "bun vite",
        "build": "rm -rf ./dist/* && bunx tsgo & bun vite build",
        "preview": "bun vite preview",
        "icons:build-modules": "bun run ../../bin/addIconFiles.ts -t './bin/reactIconTemplate.ts' -out './src/icons' -i './src/icons.ts' --format ts --clean --lib react",
        "ci": "bun run icons:build-modules && bun run build"
    },
    "author": "ProCode Software",
    "license": "MIT",
    "bugs": "https://github.com/ProCode-Software/proicons/issues",
    "repository": {
        "url": "ProCode-Software/proicons",
        "directory": "packages/proicons-react"
    },
    "homepage": "https://github.com/ProCode-Software/proicons/tree/main/packages/proicons-react",
    "keywords": [
        "icons",
        "icon-pack",
        "js",
        "feather",
        "lucide",
        "fluent",
        "font awesome",
        "svg",
        "proicons",
        "procode",
        "react"
    ],
    "devDependencies": {
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@vitejs/plugin-react-swc": "^3.11.0",
        "@typescript/native-preview": "7.0.0-dev.20250723.1",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "typescript": "^5.9.3",
        "vite": "npm:rolldown-vite@^7.1.13",
        "@proicons/shared": "workspace:*"
    },
    "peerDependencies": {
        "react": "^19.2.0"
    }
}
