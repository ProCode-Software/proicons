{
    "name": "@proicons/react",
    "version": "4.11.3",
    "description": "ProIcons icon library for ReactJS",
    "main": "dist/proicons-react.js",
    "module": "dist/proicons-react.js",
    "types": "dist/proicons-react.d.ts",
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist", "!dist/types"
    ],
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "dev": "pnpm vite",
        "build": "rm -rf ./dist/* && tsc && pnpm vite build && pnpm run build:types",
        "build:types": "dts-bundle-generator dist/types/packages/proicons-react/src/proicons-react.d.ts -o dist/proicons.d.ts --no-banner && rm -rf ./dist/types",
        "preview": "pnpm vite preview",
        "icons:build-modules": "node ../../bin/addIconFiles.js -t './bin/reactIconTemplate.js' -out './src/icons' -i './src/icons.ts' --format ts --clean --lib react",
        "ci": "pnpm run icons:build-modules && pnpm run build"
    },
    "author": "ProCode Software",
    "license": "MIT",
    "bugs": "https://github.com/ProCode-Software/proicons/issues",
    "repository": {
        "url": "ProCode-Software/proicons",
        "directory": "packages/proicons-react"
    },
    "homepage": "https://github.com/ProCode-Software/proicons/tree/main/packages/proicons-react",
    "keywords": [
        "icons",
        "icon-pack",
        "js",
        "feather",
        "lucide",
        "fluent",
        "font awesome",
        "svg",
        "proicons",
        "procode",
        "react"
    ],
    "devDependencies": {
        "@types/react": "^18.3.16",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react-swc": "^3.7.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "typescript": "^5.7.2",
        "vite": "^5.4.11",
        "vite-plugin-dts": "^4.3.0",
        "dts-bundle-generator": "^9.5.1",
        "rollup-plugin-dts": "^6.1.1"
    },
    "peerDependencies": {
        "react": "^18.3.1"
    }
}
