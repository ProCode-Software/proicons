{
    "name": "proicons",
    "version": "4.13.0",
    "description": "A collection of 500+ modern and open-source icons",
    "type": "module",
    "sideEffects": false,
    "main": "dist/cjs/proicons.cjs",
    "main:umd": "dist/umd/proicons.cjs",
    "module": "dist/esm/proicons.js",
    "typings": "dist/proicons.d.ts",
    "amdName": "proicons",
    "unpkg": "dist/umd/proicons.cjs",
    "jsdelivr": "dist/umd/proicons.cjs",
    "browser": "dist/umd/proicons.cjs",
    "workspaces": [
        "packages/*",
        "tools/*",
        "site"
    ],
    "files": [
        "dist",
        "!dist/types"
    ],
    "exports": {
        ".": {
            "types": "./dist/types/proicons.d.ts",
            "import": "./dist/esm/proicons.js",
            "require": "./dist/cjs/proicons.cjs",
            "default": "./dist/umd/proicons.cjs"
        }
    },
    "repository": {
        "url": "ProCode-Software/proicons"
    },
    "publishConfig": {
        "access": "public"
    },
    "homepage": "https://github.com/ProCode-Software/proicons",
    "bugs": "https://github.com/ProCode-Software/proicons/issues",
    "scripts": {
        "dev": "bun vite",
        "build": "bun vite build",
        "icons:build-modules": "bun run ../../bin/addIconFiles.ts -t './bin/build/templates/iconTemplate.ts' -out './src/icons' -i './src/icons.ts' -d --format ts --clean",
        "ci": "bun run icons:build-modules && bun run build"
    },
    "keywords": [
        "icons",
        "icon-pack",
        "js",
        "feather",
        "lucide",
        "fluent",
        "font awesome",
        "svg",
        "proicons",
        "procode",
        "icon-library"
    ],
    "author": "ProCode Software",
    "license": "MIT",
    "devDependencies": {
        "@proicons/shared": "workspace:*",
        "@types/bun": "latest",
        "@typescript/native-preview": "7.0.0-dev.20250723.1",
        "typescript": "^7.0.0-dev.20251121.1",
        "vite": "npm:rolldown-vite@^7.2.7"
    },
    "packageManager": "bun@10.17.1+sha512.17c560fca4867ae9473a3899ad84a88334914f379be46d455cbf92e5cf4b39d34985d452d2583baf19967fa76cb5c17bc9e245529d0b98745721aa7200ecaf7a"
}
