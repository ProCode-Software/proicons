{
    "name": "proicons-svelte",
    "private": true,
    "version": "4.11.2",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "package": "svelte-package --input ./src",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json && tsc -p tsconfig.node.json",
        "icons:build-modules": "node ../../bin/addIconFiles.js -t './bin/svelteIconTemplate.js' -out './src/icons' -i './src/icons.ts' --format ts --clean --lib svelte",
        "ci": "pnpm run icons:build-modules && pnpm run build"
    },
    "devDependencies": {
        "@sveltejs/vite-plugin-svelte": "^5.0.0",
        "@tsconfig/svelte": "^5.0.4",
        "svelte-check": "^4.1.0",
        "tslib": "^2.8.1",
        "typescript": "~5.6.2",
        "vite": "^6.0.1",
        "vite-plugin-dts": "4.3.0",
        "@sveltejs/package": "2.3.7"
    },
    "peerDependencies": {
        "svelte": "^5.2.7"
    }
}