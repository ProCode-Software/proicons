{
    "compilerOptions": {
        "moduleResolution": "Bundler",
        "module": "ESNext",
        "lib": [ "ESNext" ],
        "target": "ESNext",
        "pretty": true,
        "moduleDetection": "force",
        // Lint
        "strict": false,
        "noImplicitAny": false,
        "noUnusedParameters": false,
        "skipDefaultLibCheck": true,
        "skipLibCheck": true,
        "rewriteRelativeImportExtensions": true
    },
}